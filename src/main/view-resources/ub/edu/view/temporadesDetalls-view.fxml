<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefHeight="600.0" prefWidth="350.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ub.edu.view.EscenaTemporadesDetalls">
   <children>
   
      <AnchorPane id="anchorpane" minHeight="0.0" minWidth="0.0" prefHeight="500.0" prefWidth="330.0">
          <children>
              <!-- Titulo -->
              <Text layoutX="79.0" layoutY="38.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Temporades">
                  <font>
                  <Font size="35.0" />
               </font>
              </Text>
   
              <!-- Scroll de los Tram-->
              <ScrollPane layoutX="30.0" layoutY="50.0" prefHeight="400.0" prefWidth="300.0">
                  <content>
                      <AnchorPane fx:id="temporada_pane" minHeight="0.0" minWidth="0.0" prefHeight="398.0" prefWidth="298.0">
                          <children>
                              <Button fx:id="temporada_btn" layoutX="11.0" layoutY="14.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="275.0" text="Temporada Detail" />
                          </children>
                      </AnchorPane>
                  </content>
              </ScrollPane>
          </children>
      </AnchorPane>
        <Button fx:id="addToWatched" mnemonicParsing="false" onAction="#onBtnWatchedHistoryAddClick" prefHeight="32.0" prefWidth="200.0" text="Add to Watched History" />
   </children>
</VBox>
